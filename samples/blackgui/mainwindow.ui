    /**  border-radius: 20px; **/
    background-color: darkslategray;
    margin: 0px;
    padding: 5px;
}

#fr_CentralFrameInside {
    background-color: darkslategray;
    margin: 5px;
}

#wi_MainKeypadArea {
    background-color: darkslategray;
}

#gb_AtcStationsOnlineInfo {
